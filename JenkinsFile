pipeline {
	agent none
	stages {
		stage('restore') {
			agent { label 'windows' }
			steps {
				bat "IF EXIST \"c:/build/SimpleWebApp\" RMDIR /S /Q \"c:/build/SimpleWebApp\""
				bat 'nuget restore sample-dotnetframework-app/SampleNetApp/SampleApplication.sln'
				bat "\"${tool 'msbuild'}\"  sample-dotnetframework-app/SampleNetApp/SampleApplication.sln /p:Configuration=Release /p:Platform=\"Any CPU\" /nr:false /m /p:OutDir=c:\\build\\fassnotary"
			}
		}
		
	
	}

	post {
		always {
			echo 'testing'
		}
		success {
			echo 'successful run :)'
		}
		failure {
			echo 'Failure :('
		}
		unstable {
			echo 'unstable :O'
		}
		changed {
			echo 'chaned :]'
		}
	
	}
}