pipeline {
	agent none
	stages {
		stage('restore') {
			agent { label 'windows' }
			steps {
				bat 'nuget restore sample-dotnetframework-app/SampleNetApp/SampleApplication.sln'
			}
		}
		
	
	}

	post {
		always {
			echo 'testing'
		}
		success {
			echo 'successful run :)'
		}
		failure {
			echo 'Failure :('
		}
		unstable {
			echo 'unstable :O'
		}
		changed {
			echo 'chaned :]'
		}
	
	}
}