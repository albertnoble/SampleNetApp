pipeline {
	agent any
	
	stages('restore') {
		steps {
			bat 'nuget restore C:/Users/SirAl/jenkins/sample-dotnetframework-app/SampleNetApp/SampleApplication.sln'
			bat "\"${tool 'windows 2019'\" C:/Users/SirAl/jenkins/sample-dotnetframework-app/SampleNetApp/SampleApplication.sln /p:Configuation=Release /pPlatform=\"Any CPU\" /p:ProductVersion=1.0.0.0 /p:GenerateBuildInfoConfigFile=false /p:GenerateProjectSpecificOutputFolder=True /p:OutputPath=Bin /p:langversion=latest"
		}
	
	}

	post {
		always {
			echo 'testing'
		}
		success {
			echo 'successful run :)'
		}
		failure {
			echo 'Failure :('
		}
		unstable {
			echo 'unstable :O'
		}
		changed {
			echo 'chaned :]'
		}
	
	}
}